alias duh='du -sh'
alias findd='find . -type d'
alias findf='find . -type f'   
alias findfg='find . -type f|grep' 
alias fnm='find . -name'
alias l='ls -lrt'       
alias l.='ls -d .* --color=auto'      
alias ll='ls -l --color=auto'      
alias ls='ls --color=auto'      
alias lsh='l |head'     
alias lst='l |tail'     
alias lwc='l |wc'     
alias psa='ps -ef| grep ${USER} | grep -v grep'     
alias psg='ps -ef| grep '     
alias wct='wc *|tail'     
